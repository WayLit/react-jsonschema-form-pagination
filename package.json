{
  "name": "@waylit/react-jsonschema-form-pagination",
  "description": "Extension of react-jsonschema-form with support for multi-nav forms. Fork of https://github.com/RXNT/react-jsonschema-form-pagination",
  "private": false,
  "author": "satya@waylit.com",
  "version": "0.5.0",
  "scripts": {
    "build:lib": "rimraf lib && cross-env NODE_ENV=production babel -d lib/ src/",
    "build:dist": "rimraf dist && cross-env NODE_ENV=production webpack --config webpack.config.dist.js --optimize-minimize --display-modules",
    "build:playground": "rimraf build && cross-env NODE_ENV=production webpack --config webpack.config.prod.js --optimize-minimize && cp playground/index.prod.html build/index.html",
    "cs-check": "prettier -l $npm_package_prettierOptions '{playground,src,test}/**/*.js'",
    "cs-format": "prettier $npm_package_prettierOptions '{playground,src,test}/**/*.js' --write",
    "dist": "npm run build:lib && npm run build:dist",
    "lint": "eslint src test playground --fix",
    "precommit": "lint-staged",
    "publish-to-gh-pages": "npm run build:playground && gh-pages --dist build/",
    "publish-to-npm": "npm run dist &&  npm publish && npm version patch",
    "start": "webpack-dev-server",
    "tdd": "jest --watchAll",
    "test": "jest"
  },
  "jest": {
    "verbose": true,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ]
  },
  "prettierOptions": "--jsx-bracket-same-line --trailing-comma es5 --semi",
  "lint-staged": {
    "{playground,src,test}/**/*.js": [
      "npm run lint",
      "npm run cs-format",
      "git add"
    ]
  },
  "main": "lib/index.js",
  "files": [
    "dist",
    "lib"
  ],
  "engineStrict": false,
  "engines": {
    "node": ">=8"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.8",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/register": "^7.17.7",
    "@testing-library/dom": "^8.11.3",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.4",
    "@testing-library/user-event": "^13.5.0",
    "atob": "^2.1.2",
    "babel-jest": "^27.5.1",
    "babel-loader": "^8.2.4",
    "babel-polyfill": "^6.26.0",
    "coveralls": "^3.1.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.1.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.29.4",
    "exit-hook": "^3.0.0",
    "express": "^4.17.3",
    "gh-pages": "^3.2.3",
    "has-flag": "^5.0.1",
    "html": "1.0.0",
    "jest": "^27.5.1",
    "jsdom": "^19.0.0",
    "lint-staged": "^12.3.7",
    "mini-css-extract-plugin": "^2.6.0",
    "prettier": "^2.6.0",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-jsonschema-form": "^1.8.1",
    "react-test-renderer": "^17.0.2",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "regenerator-runtime": "^0.13.9",
    "rimraf": "^3.0.2",
    "sinon": "^13.0.1",
    "style-loader": "^3.3.1",
    "typescript": "^4.6.2",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4",
    "webpack-hot-middleware": "^2.25.1"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RxNT/react-jsonschema-form-conditionals.git"
  },
  "keywords": [
    "react",
    "form",
    "json-schema",
    "conditional",
    "predicate"
  ],
  "license": "Apache-2.0",
  "homepage": "https://github.com/RxNT/react-jsonschema-form-conditionals#readme"
}
